! font {{{
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: 1
Xft.hinting: 0
Xft.antialias: 1
Xft.rgba: rgb
! }}}
! rofi {{{
rofi.color-enabled: true
rofi.matching: fuzzy
rofi.combi-modi: window,drun,ssh
rofi.separator-style: none
rofi.hide-scrollbar: true
rofi.scroll-method: 1
rofi.modi: window,run,ssh,combi
rofi.line-margin: 0
rofi.line-padding: 8
rofi.window-format: {c} {t}
rofi.theme: squared-nord
! }}}
! urxvt {{{
urxvt.scrollBar: false
urxvt.internalBorder: 16
urxvt.lineSpace: 1
urxvt.iso14755: false
urxvt.font: xft:Inconsolata:pixelsize=15
urxvt.boldFont:
urxvt.cursorBlink: true
! }}}
! colors {{{
#define white #b7bec9
#define black #262729
#define grey #88909f
#define red #e77171
#define green #a1bf78
#define cyan #5ebaa5
#define yellow #dbb774
#define blue #73b3e7
#define magenta #d390e7

*.foreground: white
*.background: black
*.cursorColor: white
*fading: 10
*fadeColor: black

*.color0: grey
*.color1: red
*.color2: green
*.color3: yellow
*.color4: blue
*.color5: magenta
*.color6: cyan
*.color7: white
*.color8: grey
*.color9: red
*.color10: green
*.color11: yellow
*.color12: blue
*.color13: magenta
*.color14: cyan
*.color15: white
! }}}
